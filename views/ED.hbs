<!DOCTYPE html>
<html lang="en">

<head>
    <title>PCM - Espace Demandeur</title>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1, shrink-to-fit=no" name="viewport" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,500,600,700" rel="stylesheet" />
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet"
        href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/css/bootstrap-select.min.css" />

    <link href="https://releases.transloadit.com/uppy/v2.3.3/uppy.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/open-iconic-bootstrap.min.css" />
    <link rel="stylesheet" href="/css/animate.css" />

    <link rel="stylesheet" href="/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="/css/owl.theme.default.min.css" />
    <link rel="stylesheet" href="/css/magnific-popup.css" />

    <link rel="stylesheet" href="/css/aos.css" />

    <link rel="stylesheet" href="/css/ionicons.min.css" />

    <link rel="stylesheet" href="/css/bootstrap-datepicker.css" />
    <link rel="stylesheet" href="/css/jquery.timepicker.css" />

    <link rel="stylesheet" href="/css/flaticon.css" />
    <link rel="stylesheet" href="/css/icomoon.css" />
    <link rel="stylesheet" href="/css/style.css" />
</head>

<body>
    <!-- News ticker -->

    {{> news}}

    <!-- End News Ticker -->

    <!-- nav -->

    {{> nav EspaceDemandeur='true'}}

    <!-- END nav -->
    <section class="home-slider owl-carousel">
        <div class="slider-item bread-item" style="
          background-image: url('/images/son4.jpg');
          background-position: 50% -10px;
        " data-stellar-background-ratio="0.8">
            <div class="overlay"></div>
            <div class="container" data-scrollax-parent="true">
                <div class="row slider-text align-items-end">
                    <div class="col-md-7 col-sm-12 ftco-animate mb-5">
                        <h1 style="font-weight: 900; font-size: 70px; margin-left: 250px" class="mb-3"
                            data-scrollax=" properties: { translateY: '70%', opacity: .9}">
                            Espace
                        </h1>
                        <h1 style="font-weight: 900; font-size: 70px; margin-left: 171px" class="mb-3"
                            data-scrollax=" properties: { translateY: '70%', opacity: .9}">
                            Demandeur
                        </h1>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="ftco-section contact-section ftco-degree-bg">
        <div class="container heading-section">
            <h2 class="text-capitalize">Vos demandes :</h2>
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th scope="col">Num</th>
                        <th scope="col">Bénéficier</th>
                        <th scope="col">Etablissements</th>
                        <th scope="col">Act médicale</th>
                        <th scope="col">date</th>
                        <th scope="col">type</th>
                        <th scope="col">etat</th>
                        <th scope="col">Engager</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each table }}
                    <tr>
                        <th scope="row">{{this.ID}}</th>
                        <td>{{this.DATE_AGENT1}}</td>
                        <td>{{this}}</td>
                        <td>{{this}}</td>
                        <td>{{this}}</td>
                        <td>{{this}}</td>
                        <td>
                            <label class="badge badge-warning">En cour</label>
                        </td>
                        <td>#</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
            <div class="container heading-section">
                <h2 class="text-capitalize text-center">Envoyer un demande :</h2>
                <div class="container">
                    <form action="#">
                        <h2 class="text-center">Remplissez le formulaire suivant :</h2>
                        <div class="form-check-inline ali">
                            <h6 class="mr-2 text-black">Type de demande :</h6>
                            <input id="AgentCheck" onclick="myFun(this)" name="statuAdh" style="margin-bottom: 5px"
                                type="checkbox" class="form-check-inline" placeholder="Agent" />
                            <label class="mr-2">Tiers payant</label>
                            <div class="mr-2"></div>
                            <input class="form-check-inline" id="RetCheck" name="statuAdh" onclick="myFun(this)"
                                placeholder="Agent" style="margin-bottom: 5px" type="checkbox" />
                            <label class="mr-2">Médecines de soins</label>
                            <div class="mr-2"></div>
                            <input id="VeufCheck" onclick="myFun(this)" name="statuAdh" style="margin-bottom: 5px"
                                type="checkbox" class="form-check-inline" placeholder="Agent" />
                            <label class="mr-2">Prises en charge 100 %</label>
                            <div class="mr-2"></div>
                            <input id="VeufCheck" onclick="myFun(this)" name="statuAdh" style="margin-bottom: 5px"
                                type="checkbox" class="form-check-inline" placeholder="Agent" />
                            <label class="mr-2">Randevou CMS</label>
                        </div>
                        <br />
                        <div class="form-check-inline ali">
                            <h6 class="mr-2 text-black">Statut de l’adhérent :</h6>
                            <input id="AgentCheck" onclick="myFun(this)" name="statuAdh" style="margin-bottom: 5px"
                                type="checkbox" class="form-check-inline" placeholder="Agent"
                                onchange="addFieldsAgent()" />
                            <label class="mr-2">Agent</label>
                            <div class="mr-2"></div>
                            <input id="RetCheck" onclick="myFun(this);addAgent()" name="statuAdh"
                                style="margin-bottom: 5px" type="checkbox" class="form-check-inline" placeholder="Agent"
                                onchange="addFieldsRet()" />
                            <label class="mr-2">Retraité(e)</label>
                            <div class="mr-2"></div>
                            <input id="VeufCheck" onclick="myFun(this)" name="statuAdh" style="margin-bottom: 5px"
                                type="checkbox" class="form-check-inline" placeholder="Agent"
                                onchange="addFieldsVeuf()" />
                            <label class="mr-2">veuf(ve) </label>
                        </div>
                        <div class="row block-9">
                            <div class="rowDPC d-flex mb-5 contact-info">
                                <div class="w-100"></div>
                                <div class="form-group">
                                    <input id="Agentlastname" type="text" class="form-control" placeholder="Nom" />
                                </div>
                                <div class="form-group">
                                    <input id="Agentfirstname" type="text" class="form-control" placeholder="Prenom" />
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Matricule" />
                                </div>
                            </div>
                            <div class="rowDPC d-flex mb-5 contact-info">
                                <div class="w-100"></div>
                                <div class="form-group">
                                    <input id="agent" type="text" class="form-control" placeholder="Téléphone" />
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="E-mail" />
                                </div>
                                <div style="margin-top: 20px">
                                    <select class="selectpicker form-control" data-live-search="true" title="Employeur">
                                        <optgroup label="Sonatrach">
                                            <option data-tokens="iap">
                                                Institut algérien du pétrole
                                            </option>
                                            <option data-tokens="asl">
                                                Direction des œuvres sociale
                                            </option>
                                            <option data-tokens="dp">
                                                Division Production Sonatrach
                                            </option>
                                        </optgroup>
                                        <optgroup label="extra">
                                            <option>ENAFOR</option>
                                            <option>GCB</option>
                                            <option>ENAGEO</option>
                                        </optgroup>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-check-inline ali">
                            <h6 class="mr-2 text-black">Bénéficiaire de la prestation :</h6>
                            <input id="check1" name="bene" style="margin-bottom: 5px" type="checkbox"
                                class="form-check-inline" placeholder="Agent" onclick="myFun2(this);copyTextValue();"
                                onchange="removeAd()" />
                            <label class="mr-2">L’adhérent</label>
                            <div class="mr-2"></div>
                            <input id="check2" onclick="myFun2(this);getLastTextValue(this);" name="bene"
                                style="margin-bottom: 5px" type="checkbox" class="form-check-inline" placeholder="Agent"
                                onchange="addFieldsAd();" />
                            <label class="mr-2">Ayant droit*</label>
                        </div>
                        <div class="row block-9">
                            <div class="rowDPC d-flex mb-5 contact-info">
                                <div class="w-100"></div>
                                <div class="form-group">
                                    <input id="benLastName" type="text" class="form-control" placeholder="Nom" />
                                </div>
                                <div style="margin-top: 20px">
                                    <select id="lienParentie" class="selectpicker form-control"
                                        title="lien de parenté avec l’adhérent">
                                        <option>Conjointe / Conjoint</option>
                                        <option value="fils">Fille / fils</option>
                                    </select>
                                </div>
                                <div style="margin-top: 20px">
                                    <select class="selectpicker form-control" title="Type d’acte demandé">
                                        <option>Ophtalmologie</option>
                                        <option>Oncologie</option>
                                        <option>Imagerie</option>
                                        <optgroup label="Chirurgie">
                                            <option>Obstétrique</option>
                                            <option>Cancérologie</option>
                                            <option>Autres Chirurgies</option>
                                        </optgroup>
                                    </select>
                                    <div class="form-group">
                                        <label>Prescription Médicale de l’acte demandé</label>
                                        <input type="file" class="form-control-file" />
                                    </div>
                                    <div id="leftAdh" class="form-group"></div>
                                    <div id="leftBen" class="form-group"></div>
                                </div>
                            </div>
                            <div class="rowDPC d-flex mb-5 contact-info">
                                <div class="w-100"></div>
                                <div class="form-group">
                                    <input id="benFirstName" type="text" class="form-control" placeholder="Prenom" />
                                </div>
                                <div class="form-group">
                                    <input id="date" type="date" class="form-control" placeholder="Date" />
                                </div>
                                <div style="margin-top: 20px">
                                    <select data-live-search="true" class="selectpicker form-control"
                                        title="Structure médicale conventionnée souhaitée">
                                        <option>EL SALAM</option>
                                        <option>EL HILAL</option>
                                        <option>SARL CLINIQUE NOUR EL DOUNIA</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>
                                        facture pro-forma de la prescription demandée
                                    </label>
                                    <input type="file" class="form-control-file" id="exampleFormControlFile1" />
                                </div>
                                <div id="rightAdh" class="form-group"></div>
                            </div>
                        </div>
                        <div id="fils"></div>
                        <div class="form-group">
                            <input class="btn btn-primary py-3 px-5" type="submit" value="visualise" />
                        </div>
                        <!-- Add here-->
                        <div id="drag-drop-area"></div>
                        <p>am here</p>
                    </form>
                </div>
            </div>
        </div>
    </section>

    <section class="ftco-section contact-section ftco-degree-bg"></section>

    <!-- Footer -->

    {{>footer}}

    <!-- END Footer -->


    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen">
        <svg class="circular" width="48px" height="48px">
            <circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" />
            <circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10"
                stroke="#F96D00" />
        </svg>
    </div>

    <!-- Modal -->
    <div class="modal fade" id="modalRequest" tabindex="-1" role="dialog" aria-labelledby="modalRequestLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modalRequestLabel">
                        Make an Appointment
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form action="#">
                        <div class="form-group">
                            <!-- <label for="appointment_name" class="text-black">Full Name</label> -->
                            <input type="text" class="form-control" id="appointment_name" placeholder="Full Name" />
                        </div>
                        <div class="form-group">
                            <!-- <label for="appointment_email" class="text-black">Email</label> -->
                            <input type="text" class="form-control" id="appointment_email" placeholder="Email" />
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <!-- <label for="appointment_date" class="text-black">Date</label> -->
                                    <input type="text" class="form-control appointment_date" placeholder="Date" />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <!-- <label for="appointment_time" class="text-black">Time</label> -->
                                    <input type="text" class="form-control appointment_time" placeholder="Time" />
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <!-- <label for="appointment_message" class="text-black">Message</label> -->
                            <textarea name="" id="appointment_message" class="form-control" cols="30" rows="10"
                                placeholder="Message"></textarea>
                        </div>
                        <div class="form-group">
                            <input class="btn btn-primary" type="submit" value="Make an Appointment" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="https://releases.transloadit.com/uppy/v2.3.3/uppy.min.js"></script>
    <script>
        var uppy = new Uppy.Core()
        uppy.use(Uppy.DragDrop, {
            target: '#drag-drop-area'
        })
        uppy.use(Uppy.Tus, {
            endpoint: 'https://localhost:3000/upload'
        })
    </script>
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/jquery.easing.1.3.js"></script>
    <script src="/js/jquery.waypoints.min.js"></script>
    <script src="/js/jquery.stellar.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/jquery.magnific-popup.min.js"></script>
    <script src="/js/aos.js"></script>
    <script src="/js/jquery.animateNumber.min.js"></script>
    <script src="/js/bootstrap-datepicker.js"></script>
    <script src="/js/jquery.timepicker.min.js"></script>
    <script src="/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false">
    </script>
    <script src="/js/google-map.js"></script>
    <script src="/js/main.js"></script>

    <!-- Latest compiled and minified JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/js/bootstrap-select.min.js"></script>

    <!-- (Optional) Latest compiled and minified JavaScript translation files -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/js/i18n/defaults-*.min.js"></script>
</body>

</html>